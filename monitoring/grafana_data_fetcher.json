{
  "dashboard": {
    "title": "Data Fetcher",
    "panels": [
      {
        "type": "graph",
        "title": "429 Rate",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(data_ingestion_429_total[1m]))",
            "legendFormat": "429 per sec"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Processing Steps",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum(rate(data_processing_steps_total[1m]))",
            "legendFormat": "steps per sec"
          }
        ]
      }
    ]
  }
}
